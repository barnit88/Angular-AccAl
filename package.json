{
  "name": "gotham",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:auth": "ng serve --project auth --port 4200",
    "start:dash": "ng serve --project dashboard --port 4201",
    "start:land": "ng serve --project landing --port 4202",
    "start:dashData": "ng serve --project dashboard --port 4203 --proxy-config proxy.config.json",
    "build:shared": "ng build shared",
    "build:auth": "ng build auth",
    "build:land": "ng build landing",
    "build:dash": "ng build dashboard",
    "build:shared-dev": "ng build shared --watch",
    "build": "npm run build:shared && npm run build:auth && npm run build:land && npm run build:dash",
    "start": "concurrently \"npm run start:auth\" \"npm run start:dash\" \"npm run start:land\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier .",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "pretty-quick": "pretty-quick"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.8",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fontsource/russo-one": "^4.5.10",
    "@fortawesome/angular-fontawesome": "^0.12.1",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@ngrx/effects": "^15.3.0",
    "@ngrx/entity": "^15.3.0",
    "@ngrx/router-store": "^15.3.0",
    "@ngrx/store": "^15.3.0",
    "@ngrx/store-devtools": "^15.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "angular-datatables": "^15.0.0",
    "axios": "^1.3.5",
    "chart.js": "^4.2.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "datatables.net": "^1.13.4",
    "datatables.net-buttons": "^2.4.2",
    "datatables.net-buttons-dt": "^2.4.2",
    "datatables.net-dt": "^1.13.4",
    "dayjs": "^1.11.9",
    "firebase": "^9.22.0",
    "jquery": "^3.6.4",
    "jszip": "^3.10.1",
    "lint-staged": "^13.2.1",
    "lodash": "^4.17.21",
    "ng-select2-component": "^11.0.0",
    "ng2-charts": "^4.1.1",
    "ngx-cookie-service": "^15.0.0",
    "ngx-toastr": "^16.1.0",
    "rxjs": "^7.8.0",
    "shared": "file:dist/shared",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.4",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular/cli": "~15.2.1",
    "@angular/compiler-cli": "^15.2.0",
    "@types/datatables.net": "^1.10.24",
    "@types/datatables.net-buttons": "^1.4.9",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.16",
    "@types/lodash": "^4.14.194",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.2.2",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.9.4"
  },
  "lint-staged": {
    "*.ts": [
      "eslint . --quiet --fix --",
      "pretty-quick --staged"
    ],
    "*.{scss,css,yaml,yml,md,mdx,mjs}": [
      "pretty-quick --staged"
    ]
  }
}
